pipeline {
	agent any
	stages {
		stage ('BUILD') {
			steps {
				sh echo 'running BUILD phase'
				}
		}
		stage ('TEST') {
			steps {
				sh echo 'running TEST phase'
				}
		}
		stage ('QA') {
			steps {
				sh echo "running QA phase"
				}
		}
		stage ('DEPLOY') {
			steps {
				sh echo "running DEPLOY phase"
				}
		}
		stage ('MONITOR') {
			steps {
				sh echo "running MONITOR phase"
				}
		}
	}
}
